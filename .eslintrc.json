{
  "extends": [
    "airbnb-base",
    "plugin:vue/essential"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "overrides": [
    {
      "files": ["*.js", "*.vue"],
      "rules": {
        "indent": "off",
        "no-new": "off",
        "quote-props": "off",
        "vue/script-indent": ["error", 4, { "baseIndent": 1 }]
      }
    }
  ]
}
